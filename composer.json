{
    "name": "ecloud-apio",
    "description": "eCloud APIo Webservice",
    "keywords": ["ukfast", "apio", "ecloud", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "ukfast/api-resources": "^3.1.0",
        "ukfast/api-exceptions": "^2.2",
        "ukfast/api-helper-datetime": "^2.0.1",
        "ukfast/api-auth": "^2.1",
        "ukfast/api-paginator": "^2.0",
        "ukfast/ditto": "4.1.*",
        "ukfast/api-validation": "~1.0",
        "ukfast/log-provider": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "^2.9",
        "phpmd/phpmd": "^2.6.0",
        "ukfast/rulesets": "^1.0.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
        "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/resources.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/exceptions.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/helpers/date-time-helper.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/authentication.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/paginator.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/db/ditto.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:ukfast/api.ukfast/packages/php/log-provider.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/rulesets.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:ukfast/api.ukfast/packages/php/validation.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.devops.ukfast.co.uk:rnd/api.ukfast/packages/php/helpers/encryption-helper.git"
        }
    ]
}
